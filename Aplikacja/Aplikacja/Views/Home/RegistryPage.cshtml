<h3>@ViewBag.Message</h3>



<div class="wrapper">
    <form class="form-signin">
        <h2 class="form-signin-heading">Zarejestruj swoje konto</h2>
        <input type="text" class="form-control" name="username" placeholder="Login Name" required="" autofocus="" />
        <input type="text" class="form-control" name="email" placeholder="Email" required="" />
        <input type="password" class="form-control" name="password" placeholder="Password" required="" />
        <input type="text" class="form-control" name="Country" placeholder="Country" required="" />
        <label class="checkbox">
            <input type="checkbox" value="remember-me" id="rememberMe" name="rememberMe"> Remember me
        </label>
        <a href="loginsucces.html"><button class="btn btn-lg btn-primary btn-block" type="submit">Register</button></a>
    </form>
</div>

